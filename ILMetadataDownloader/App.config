<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <appSettings>
    <add key="DEBUG_MODE" value="false" />
    <add key="MAIN_LOOP_INTERVAL" value="999" />
    <add key="TORRENT_PARALLEL_LIMIT" value="33" />
    <add key="TORRENT_STOP_TIMEOUT" value="3" />
    <!-- Save path of metadata to torrent files -->
    <add key="TORRENT_OUTPUT_PATH" value="c:\tmp_out\" />
    <!-- Skip torrent files who are less than .. -->
    <add key="TORRENT_MIN_FILE_SIZE_MB" value="888" />
    <add key="TMP_SAVE_DIR" value=".\tmp_dld\" />
    <add key="MAGNET_PREFIX" value="magnet:?xt=urn:btih:" />
    <!-- hashes DB -->
    <add key="SDB_URL" value="c:\tmp1\MTorr.db" />
    <!-- already downloaded torrents -->
    <add key="SDB_DLD_URL" value="c:\tmp1\MTorrLL.db" />
    <add key="BAN_WORDS_FILE" value="ban_words.txt" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
</configuration>
